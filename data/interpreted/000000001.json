{
  "id": "1.03",
  "url": "https://www.revisor.mn.gov/statutes/cite/1.03",
  "title": "1.03 WATERS INCLUDED.",
  "text": "The concurrent jurisdiction of a county and of courts and officers exercising jurisdiction throughout it extends over the water area that would be included if the boundary lines of the county were produced in the direction of their approach and extended across the waters to the opposite shore.",
  "repealed": false,
  "rule": {
    "and": [
      {
        "==": [
          {
            "var": "jurisdiction_type"
          },
          "concurrent"
        ]
      },
      {
        "==": [
          {
            "var": "area_type"
          },
          "water"
        ]
      },
      {
        "==": [
          {
            "var": "boundary_extension"
          },
          "produced"
        ]
      },
      {
        "==": [
          {
            "var": "boundary_extension_direction"
          },
          "towards_opposite_shore"
        ]
      }
    ]
  },
  "examples": [
    {
      "jurisdiction_type": "concurrent",
      "area_type": "water",
      "boundary_extension": "produced",
      "boundary_extension_direction": "towards_opposite_shore"
    },
    {
      "jurisdiction_type": "exclusive",
      "area_type": "land",
      "boundary_extension": "not_produced",
      "boundary_extension_direction": "away_from_shore"
    },
    {
      "jurisdiction_type": "concurrent",
      "area_type": "water",
      "boundary_extension": "not_produced",
      "boundary_extension_direction": "towards_opposite_shore"
    }
  ],
  "variables": [
    "jurisdiction_type",
    "area_type",
    "boundary_extension",
    "boundary_extension_direction"
  ],
  "consequences": [
    "County and courts have jurisdiction over the extended water area."
  ]
}